{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@dnd-history/environment": [
        "apps/dnd-history/src/environments/environment.ts"
      ],
      "@web-portfolio/dnd-history-adventure": [
        "libs/dnd-history/feature/adventure/src/index.ts"
      ],
      "@web-portfolio/dnd-history-characters": [
        "libs/dnd-history/feature/characters/src/index.ts"
      ],
      "@web-portfolio/dnd-history-header": [
        "libs/dnd-history/feature/header/src/index.ts"
      ],
      "@web-portfolio/dnd-history-home": [
        "libs/dnd-history/feature/home/src/index.ts"
      ],
      "@web-portfolio/dnd-history-login": [
        "libs/dnd-history/feature/login/src/index.ts"
      ],
      "@web-portfolio/dnd-history-map": [
        "libs/dnd-history/feature/map/src/index.ts"
      ],
      "@web-portfolio/dnd-history/client-services": [
        "libs/dnd-history/client-services/src/index.ts"
      ],
      "@web-portfolio/dnd-history/data-access/api-interfaces": [
        "libs/dnd-history/data-access/api-interfaces/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
